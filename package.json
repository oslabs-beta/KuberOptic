{
  "name": "kuberoptic",
  "version": "1.0.0",
  "productName": "KuberOptic",
  "description": "An Electron app for developers to visualize their Kubernetes clusters in real-time",
  "main": "dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "electron ./dist/main.bundle.js",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "build": "tsc && cp ./src/client/index.html",
    "build:watch": "npm run build -- --watch",
    "build:mwl": "npm run compile && electron-builder",
    "lint": "eslint --ignore-path .gitignore '**/*.{ts,tsx}' ",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "package-mac": "electron-packager . kuberoptic --overwrite --platform=darwin --arch=x64 --icon=src/client/assets/credsPage/KuberOpticIcon.icns --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/kuberoptic-darwin-x64/kuberoptic.app kuberoptic --out=release-builds --overwrite",
    "format": "prettier --write \"**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/KuberOptic.git"
  },
  "contributors": [
    "Bryan",
    "Darren",
